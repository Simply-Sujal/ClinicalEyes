{
    "version": 2,
    "builds": [
        {
            "src": "requirements.txt",
            "use": "@vercel/python"
        },
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ]
}